generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl"]
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:docker@localhost:5432/micro_db?schema=public"
}

model SalesItem {
  id       Int    @id @default(autoincrement())
  name     String
  quantity Int
  price    Float
  code     String @unique
}
